.header{box-shadow:0 4px 4px 0 rgba(0,0,0,.25);z-index:201;position:relative}.header-top{background:#414c5c}.header .container{max-width:1310px}.header .text-green{color:#d1ea8d}.header .fw-700{font-weight:700}@media(min-width: 62em){.header-logo-menu{padding:0.875rem 0}}@media(max-width: 61.98em){.header .navbar.navbar-dark{padding:1.3125rem 0}}@media(max-width: 61.98em){.header .navbar-collapse{position:absolute;top:100%;left:0;right:0;background:#fff;z-index:99;padding:0 1rem;height:calc(100vh - 82px);overflow-y:scroll;overflow-x:hidden}}@media(min-width: 62em){.header .navbar-collapse .menu-utility,.header .navbar-collapse .menu-eyebrow{display:none}}.header .navbar-toggler{cursor:pointer;border:0 none;background:rgba(0,0,0,0);outline:none;box-shadow:none;transition:all .3s ease;padding:0}.header .navbar-toggler .icon{width:28px;height:20px;position:relative;margin-left:0.625rem}.header .navbar-toggler .navbar-toggler-icon{background:#414c5c;height:4px;position:absolute;width:100%;left:0;transition:all .3s ease;border-radius:5px}.header .navbar-toggler .navbar-toggler-icon:first-child{top:0}.header .navbar-toggler .navbar-toggler-icon:nth-child(2){top:8px}.header .navbar-toggler .navbar-toggler-icon:last-child{top:16px}.header .navbar-toggler[aria-expanded=true] .icon{width:28px}.header .navbar-toggler[aria-expanded=true] .icon span:nth-child(2){opacity:0}.header .navbar-toggler[aria-expanded=true] .icon span:first-child,.header .navbar-toggler[aria-expanded=true] .icon span:last-child{top:8px}.header .navbar-toggler[aria-expanded=true] .icon span:first-child{transform:rotate(45deg)}.header .navbar-toggler[aria-expanded=true] .icon span:last-child{transform:rotate(-45deg)}.header--orphan .top-menu .nav-item:nth-child(n+2){display:none}.header--orphan .top-menu .nav-item:last-of-type{display:list-item}
